{
  "version": 2,
  "buildCommand": "pnpm install --no-frozen-lockfile && pnpm run build",
  "build": {
    "env": {
      "PUPPETEER_SKIP_DOWNLOAD": "true",
      "PUPPETEER_CACHE_DIR": "/tmp/puppeteer-cache",
      "CHROME_PATH": "/usr/bin/chromium-browser"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
